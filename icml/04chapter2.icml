		<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/$ID/[No paragraph style]">
			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Default" FontStyle="Light" PointSize="9" Ligatures="false" Tracking="-5" AppliedLanguage="$ID/English: USA" DiacriticPosition="OpentypePosition">
				<Properties>
					<AppliedFont type="string">Trade Gothic Next LT Pro</AppliedFont>
