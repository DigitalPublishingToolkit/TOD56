              <PathPointType Anchor="730.32000 425.16000" LeftDirection="730.32000 425.16000" RightDirection="730.32000 425.16000" />
              <PathPointType Anchor="730.32000 -425.16000" LeftDirection="730.32000 -425.16000" RightDirection="730.32000 -425.16000" />
            </PathPointArray>
          </GeometryPathType>
