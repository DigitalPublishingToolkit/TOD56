              <PathPointType Anchor="-1313.64000 425.16000" LeftDirection="-1313.64000 425.16000" RightDirection="-1313.64000 425.16000" />
              <PathPointType Anchor="1313.64000 425.16000" LeftDirection="1313.64000 425.16000" RightDirection="1313.64000 425.16000" />
              <PathPointType Anchor="1313.64000 -425.16000" LeftDirection="1313.64000 -425.16000" RightDirection="1313.64000 -425.16000" />
            </PathPointArray>
